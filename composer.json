{
    "name": "manageitwa/laravel-mailtrap-driver",
    "description": "Provides email support for the Mailtrap.io Email Sending Service through API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Thomson",
            "email": "ben@manageit.com.au"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0",
        "illuminate/mail": "^6.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ManageItWA\\LaravelMailtrapDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ManageItWA\\LaravelMailtrapDriver\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ManageItWA\\LaravelMailtrapDriver\\MailtrapServiceProvider"
            ]
        }
    }
}
